// reverse a number
import java.util.*;

class revnum
{
	public static void main(String args[])
	{
		int num,rev=0,rem;
		Scanner obj = new Scanner(System.in);
		num=obj.nextInt();
		while(num != 0)   
		{  
			rem = num % 10;  
			rev= rev*10+rem;  
			num= num/10;  
		}  
		System.out.println("reverse number= " + rev);  
	}
}
