// sum of even
import java.util.*;

class soo
{
	public static void main(String args[])
	{
		int i=1,s=0,n;
		Scanner obj=new Scanner(System.in);
		System.out.println("enter limit=");
		n=obj.nextInt();
		do
		{
			i+=2;
			s+=i;
		}
		while(i<n);
		System.out.println("Sum of even numbers="+s);
	}
}
